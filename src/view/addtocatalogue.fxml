<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox style="-fx-pref-width: 300; -fx-pref-height: 170;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AddToCatalogueController">

	<stylesheets>
		<URL value="@computerbuilder.css" />
	</stylesheets>
   <children>
      <HBox alignment="CENTER" prefHeight="39.0" prefWidth="280.0" style="-fx-pref-height: 40;">
         <children>
            <Label style="-fx-pref-width: 50;" text="Type:" />
            <TextField fx:id="typeField" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="41.0" prefWidth="280.0" style="-fx-pref-height: 40;">
         <children>
            <Label style="-fx-pref-width: 50;" text="Name:" />
            <TextField fx:id="nameField" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="55.0" prefWidth="280.0" style="-fx-pref-height: 40;">
         <children>
            <Label style="-fx-pref-width: 50;" text="Price:" />
            <TextField fx:id="priceField" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="48.0" prefWidth="280.0" style="-fx-pref-height: 40;">
         <children>
            <Button fx:id="addToCatalogueBtn" mnemonicParsing="false" onAction="#handleAddtoCatalogueAction" text="Add to Catalogue" />
         </children>
      </HBox>
   </children>
</VBox>
