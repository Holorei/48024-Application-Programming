<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" style="-fx-pref-width: 300; -fx-pref-height: 160;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.CheckBuildController">
   <children>
      <TextArea fx:id="textArea" prefHeight="200.0" prefWidth="200.0" />
      <Button fx:id="closeBtn" mnemonicParsing="false" onAction="#handleCloseAction" text="Close" />
   </children>
</VBox>
